<?xml version='1.0' ?>

<launch>
  <arg name="gazebo_version" default="11"/>
  <arg name="use_sim_time" default="true"/>

  <arg name="turtlebot3_model" default="waffle"/>


  <!-- gazebo launch -->
  <include file="$(find-pkg-share turtlebot3_gazebo)/turtlebot3_world.launch.py">
    <arg name="TURTLEBOT3_MODEL" value="$(var turtlebot3_model)"/>
  </include>
  
  <!-- navigation launch -->
  <include file="$(find-pkg-share turtlebot3_navigation2)/navigation2.launch.py">
    <arg name="use_sim_time" value="$(var use_sim_time)"/>
    <arg name="TURTLEBOT3_MODEL" value="$(var turtlebot3_model)"/>
  </include>

  <!-- DeliveryRobot fleet adapter -->
  <!-- <group>
    <include file="$(find-pkg-share fleet_adapter_turtlebot3)/launch/fleet_adapter.launch.xml">
      <arg name="use_sim_time" value="$(var use_sim_time)"/>
      <arg name="nav_graph_file" value="$(find-pkg-share rmf_demos_maps)/maps/glhd/nav_graphs/0.yaml" />
      <arg name="config_file" value="$(find-pkg-share rmf_demos)/config/glhd/deliveryRobot_config.yaml"/>
    </include>
  </group> -->

</launch>
